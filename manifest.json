{
  "manifest_version": 2,
  "version": "5.0.2",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "http://www.youtube.com/*",
    "https://www.youtube.com/*",
    "storage"
  ],
  "content_security_policy": "script-src 'unsafe-eval' 'self' https://www.youtube.com https://www.google-analytics.com/analytics.js https://ssl.google-analytics.com https://s.ytimg.com https://maxcdn.bootstrapcdn.com https://code.jquery.com; object-src 'self'",
  "background": {
    "scripts": [
      "analytics.js",
      "lib/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "lib/popout_for_youtube.js"
      ],
      "css": [
        "popout_for_youtube.css"
      ]
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "revjet_contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false,
      "permissions": [
        "storage"
      ]
    }
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    "options.html"
  ],
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
