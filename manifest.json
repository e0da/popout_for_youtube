{
  "background": {
    "scripts": ["build/analytics.bundle.js", "build/background.bundle.js"]
  },
  "content_scripts": [
    {
      "css": ["build/videoPage.css"],
      "js": ["build/videoPage.bundle.js"],
      "matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"]
    }
  ],
  "content_security_policy": "script-src 'unsafe-eval' 'self' https://www.youtube.com https://s.ytimg.com https://ssl.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "48": "build/images/icon48.png",
    "128": "build/images/icon128.png"
  },
  "manifest_version": 2,
  "name": "__MSG_name__",
  "permissions": [
    "tabs",
    "http://www.youtube.com/*",
    "https://www.youtube.com/*"
  ],
  "version": "6.0.0"
}
